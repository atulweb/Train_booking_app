{
    "version": 2,
    "builds": [
      {
        "src": "train-booking-frontend/package.json",
        "use": "@vercel/next",
        "config": {
          "rootDirectory": "train-booking-frontend"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "train-booking-frontend/$1"
      }
    ]
  }
  